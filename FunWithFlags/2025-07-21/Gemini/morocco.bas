10 REM MOROCCO FLAG AND ANTHEM
20 SCREEN 9 ' EGA 640x350 16 colors
30 CLS
40 KEY OFF
50 REM DRAW RED BACKGROUND
60 CRED = 4
70 LINE (0, 0)-(639, 349), CRED, BF
80 REM DRAW GREEN PENTAGRAM
90 CGREEN = 2
100 CX = 320: CY = 175: R = 80
110 DIM X(5), Y(5)
120 PI = 3.141593
130 FOR I = 1 TO 5
140   A = PI * (I * 2 / 5 - .5)
150   X(I) = CX + R * COS(A)
160   Y(I) = CY + R * SIN(A)
170 NEXT I
180 LINE (X(1), Y(1))-(X(3), Y(3)), CGREEN
190 LINE (X(3), Y(3))-(X(5), Y(5)), CGREEN
200 LINE (X(5), Y(5))-(X(2), Y(2)), CGREEN
210 LINE (X(2), Y(2))-(X(4), Y(4)), CGREEN
220 LINE (X(4), Y(4))-(X(1), Y(1)), CGREEN
230 REM PLAY "CHERIFIAN ANTHEM"
240 PLAY "T120 L8 O4"
250 PLAY "G E F G A G F E"
260 PLAY "D E F E D C D"
270 PLAY "G E F G A G F E"
280 PLAY "D C B- A- G2"
290 WHILE INKEY$ = ""
300 WEND
310 SCREEN 0
320 END
